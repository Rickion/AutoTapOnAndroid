{
  "name": "autotap-android",
  "version": "1.0.0",
  "description": "Replay your activity with your Android phone automatically.",
  "main": "index.js",
  "scripts": {
    "record": "node -e 'require(\"./src/adb-cmd.js\").record()'",
    "replay": "node index.js",
    "loop": "node index.js --loop --sec 0.5",
    "lint": "eslint --fix --ext .js index.js"
  },
  "keywords": [
    "Android",
    "auto",
    "tap",
    "replay"
  ],
  "author": "Rickion",
  "license": "GPL-3.0",
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  }
}
